<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox ">
        <div class="ibox-title">
          <div class="row">
            <div class="col-10">
              <h5>Usuarios del Sistema</h5>
            </div>
            <div class="col-2">
              <%= link_to 'Añadir Usuario', new_admin_user_path, class: "btn btn-primary float-right" %>
            </div>
          </div>
        </div>
        <div class="ibox-content">
          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dataTables clickable" data-source="<%= admin_users_path(format: :json) %>" id="ajax-datatable">
              <thead>
                <tr>
                  <th>Nombre</th>
                  <th>Apellido</th>
                  <th>Usuario</th>
                  <th>Último inicio de sesión</th>
                  <th>Estatus</th>
                </tr>
              </thead>
              <tbody></tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


