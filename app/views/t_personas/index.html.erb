<p id="notice"><%= notice %></p>

<h1>T Personas</h1>

<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Num licencia</th>
      <th>T cliente</th>
      <th>T empresa</th>
      <th>Cargo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @t_personas.each do |t_persona| %>
      <tr>
        <td><%= t_persona.cedula %></td>
        <td><%= t_persona.nombre %></td>
        <td><%= t_persona.apellido %></td>
        <td><%= t_persona.num_licencia %></td>
        <td><%= t_persona.t_cliente_id %></td>
        <td><%= t_persona.t_empresa_id %></td>
        <td><%= t_persona.cargo %></td>
        <td><%= link_to 'Show', t_persona %></td>
        <td><%= link_to 'Edit', edit_t_persona_path(t_persona) %></td>
        <td><%= link_to 'Destroy', t_persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New T Persona', new_t_persona_path %>
