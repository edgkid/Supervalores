<tr class="fields" style="background-color: #1AB394; color: aquamarine;">
  <td><input class="deletable deletable-servicio checkbox checkbox-success" type="checkbox" name="deletable"></td>
  <td>
    <%= f.select :t_tarifa_servicio_id, options_for_select(TTarifaServicio.all.map {|s| [s.nombre, s.id, {'data-id': s.id, 'data-nombre': s.nombre, 'data-descripcion': s.descripcion, 'data-value': s.precio}]}), {include_blank: 'Seleccione'}, {class: 'select2 select2-dynamic select-servicio', style: 'width: 100%'} %>
    <input class="service-descripcion" name="dynamic_attributes[t_tarifa_servicio][descripcions][]" type="hidden">
    <input class="service-id" name="dynamic_attributes[t_tarifa_servicio][ids][]" type="hidden">
  </td>
  <td class="descripcion-cell">
    <%= f.text_field :cuenta_desc, style: 'width: 100%;' %>
  </td>
  <td class="quantity-cell">
    <%= f.number_field :cantidad, 'data-quantity': 1, min: 0, class: 'quantity', style: 'height: 28px; width: 100%;', value: 1 %>
    <input class="service-quantity" name="dynamic_attributes[t_tarifa_servicio][quantities][]" type="hidden">
  </td>
  <td class="price-cell">
    <%= f.number_field :precio_unitario, min: 0, step: 0.01, class: 'price', style: 'height: 28px; width: 100%;', value: 0 %>
    <input class="service-price" name="dynamic_attributes[t_tarifa_servicio][prices][]" type="hidden">
  </td>
  <td class="subtotal-cell" style="text-align: center;">
    0
    <%= f.hidden_field :_destroy %>
    <%= link_to_remove_fields 'Eliminar', f, true, class: 'destroy-service-link', style: 'display: none;' %>
  </td>
</tr>
