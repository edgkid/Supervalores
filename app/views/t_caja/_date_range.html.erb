<%= form_with(url: t_caja_index_path, method: "get", format: 'json') do %>
  <div class="row">
    <div align="right" class="col-md-2">
      <div class="form-group">
        <%= button_tag 'Limpiar', class: 'btn btn-default', id: 'clean-dates' %>
      </div>
    </div>
    <div align="right" class="col-md-5">
      <div class="form-group">
        <%= text_field_tag 'from', nil, class: 'datepicker', placeholder: 'Desde', onchange: 'Rails.fire(this.form, "submit");', readonly: true %>
      </div>
    </div>
    <div class="col-md-5">
      <div class="form-group">
        <%= text_field_tag 'to', nil, class: 'datepicker', placeholder: 'Hasta', onchange: 'Rails.fire(this.form, "submit");', readonly: true %>
      </div>
    </div>
  </div>
<% end %>
